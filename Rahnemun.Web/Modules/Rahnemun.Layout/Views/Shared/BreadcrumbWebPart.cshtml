@model IList<SiteMapNodeViewModel>
           
@{
    var currentNodeInfo = Model.LastOrDefault();
}

@if (Model.Any())
{
    <ul>
        @for (int i = 0; i < Model.Count - 1; i++)
        {
            var nodeInfo = Model[i];
            <li><a title="@nodeInfo.Description" href="@nodeInfo.Url">@nodeInfo.Title</a></li>
        }
       
        <li class="current" title="@currentNodeInfo.Description">@currentNodeInfo.Title</li>
    </ul>
}